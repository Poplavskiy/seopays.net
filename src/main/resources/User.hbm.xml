<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="seopays.domain">
	<class name="User" table="users">
		<id name="username" type="string" length="50"></id>

		<property name="password" column="password" type="string" length="50"></property>
		<property name="enabled" column="enabled" type="boolean"></property>
	</class>
</hibernate-mapping>